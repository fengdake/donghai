        this.PassShow = false
        this.notPassShow = true
        this.chenshu = true
        this.xiuGaiTishi = '系统审核结论为通过，人工修改为未通过'
        if (this.value == this.whsjvBr.yxtshjl) {
          this.PassShow = false
          this.notPassShow = false
          this.chenshu = false
          this.xiuGaiTishi = ''
        }
