    //保存我行数据  修改系统审核结论
    PreservationWoHang() {
      this.loading = true
      this.spinning = true
      console.log(this.xiuGaiTishi)
      if (this.whsjvBr.hzgx == null || this.whsjvBr.hzgx == '') {
        this.loading = false
        this.spinning = false
        this.$message.warning('请选择合作关系')
        return false
      }
      if (this.chenshu == true) {
        if (this.whsjvBr.cs == '' || this.whsjvBr.cs == null) {
          this.loading = false
          this.spinning = false
          this.$message.warning('请输入陈述')
          return false
        }
      }
      let obj = {
        id: this.whsjvBr.id,
        sxid: this.whsjvBr.sxid,
        xtshjl: this.value,
        whblyycs: this.whsjvBr.whblyycs,
        cs: this.whsjvBr.cs,
        description: this.xiuGaiTishi,
        hzgx: this.whsjvBr.hzgx
      }
      putAction('/business/sxsqWhkhxx/editSys', obj)
        .then(res => {
          if (res.success == true) {
            this.$notification.success({
              message: '提示',
              description: `修改成功`,
              duration: 3
            })
          } else {
            // 接口失败
            this.$notification.error({
              message: '提示',
              description: `修改失败`,
              duration: 3
            })
          }
        })
        .finally(() => {
          this.loading = false
          this.spinning = false
        })
    },
