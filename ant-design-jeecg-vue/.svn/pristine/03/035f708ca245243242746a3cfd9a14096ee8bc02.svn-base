        this.$refs.hfw.dakai() //汇法网查询
      } else if (this.biglist[e].name == '现金流') {
        this.$refs.xjla.getxjl()
      }
      this.$emit('changetitle', this.biglist[e].name)
      // this.$parent.changetitle(this.biglist[e].name);
    },
